<%= javascript_include_tag "jquery-1.10.2.min.js", "data-turbolinks-track" => true %>
<%= javascript_include_tag "superfish", "data-turbolinks-track" => true %>
<%= javascript_include_tag "bootstrap.min", "data-turbolinks-track" => true %>
<%= javascript_include_tag "retina.min", "data-turbolinks-track" => true %>

<%= javascript_include_tag "validate", "data-turbolinks-track" => true %>
<%= javascript_include_tag "jquery.placeholder", "data-turbolinks-track" => true %>
<%= javascript_include_tag "functions", "data-turbolinks-track" => true %>
<%= javascript_include_tag "jquery.fitvids", "data-turbolinks-track" => true %>

<!-- Google Maps -->
<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
<%= javascript_include_tag "mapmarker.js", "data-turbolinks-track" => true %>
<script type="text/javascript">
      //set up markers 
    var myMarkers = {"markers": [
    <%(0..(@keynotes.length-1)).each do |e| %>
        {"latitude":<%= @keynotes[e].values[0] %> , "longitude":<%=@keynotes[e].values[1] %>, "icon": "img/map-marker2.png"},
    <%end %>    
            ]
    };
    
    //set up map options
    $("#map").mapmarker({
      zoom  : 4,
      center  : 'India',
      markers : myMarkers
    });

</script>
