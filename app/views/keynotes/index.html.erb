<section id="map"></section><!-- end map-->
<!-- List of past keynotes -->
<div style="position:relative">
<div class="container" style="padding-bottom:60px">
    <div class="row">
        <div class="col-md-6">
            <%unless @future.empty? %>
            <h3>List of upcoming Keynotes</h3>
            <ul class="list_1" id="past_kn">
              <%@future.each do |f| %>
                <li><a href="#"><%= f.title %></a></li>  
              <%end %>
            </ul>
            <%end %>
         </div>
         <div class="col-md-6">
            <%unless @past.empty? %>
            <h3>List of past Keynotes</h3>
            <ul class="list_1" id="past_kn">
              <%@past.each do |p| %>
                <li><a href="#"><%= p.title %></a></li>
              <%end %>
            </ul>    
            <%end %>
         </div>
    </div>
</div>
<div class="divider_top"></div>
</div>
